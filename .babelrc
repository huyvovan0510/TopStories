{
    "presets": [
        "module:metro-react-native-babel-preset"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".ios.ts",
                    ".android.ts",
                    ".ts",
                    ".ios.tsx",
                    ".android.tsx",
                    ".jsx",
                    ".js",
                    ".json"
                ],
                "alias": {
                    "@screens": "./src/screens",
                    "@constans": "./src/constans",
                    "@api": "./src/api",
                    "@components": "./src/components",
                    "@utils": "./src/utils"
                    
                    
                }
            }
        ]
    ]
}